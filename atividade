class Pessoa:
    def _init_(self,nome,sexo,cpf):
        self.nome = nome
        self.sexo = sexo
        self.cpf = cpf

    if _name_ == "_main_":
        pessoa1 = Pessoa ("Jo√£o", "M", "123456")
